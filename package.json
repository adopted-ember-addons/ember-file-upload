{
  "name": "ember-file-upload",
  "version": "3.0.5",
  "description": "An uploader component for Ember apps",
  "keywords": [
    "ember-addon",
    "file-upload"
  ],
  "homepage": "https://adopted-ember-addons.github.io/ember-file-upload",
  "repository": "https://github.com/adopted-ember-addons/ember-file-upload",
  "license": "MIT",
  "author": {
    "name": "Alon Bukai",
    "email": "alonzorz@gmail.com",
    "url": "https://github.com/Alonski"
  },
  "contributors": [
    {
      "name": "Tim Evans"
    },
    {
      "name": "Jeldrik Hanschke",
      "url": "https://github.com/jelhan"
    },
    {
      "name": "Giles Thompson",
      "email": "me@gilesthompson.co.nz",
      "url": "http://gilesthompson.co.nz"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "postversion": "git push && git push --tags"
  },
  "resolutions": {
    "ember-cli-template-lint/**/ember-template-lint": "^2.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^2.7.0",
    "ember-angle-bracket-invocation-polyfill": "^2.0.2",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.5.1",
    "ember-cli-addon-docs": "^0.7.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-code-coverage": "^1.0.0-beta.9",
    "ember-cli-content-security-policy": "^2.0.0-1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.5",
    "ember-cli-inject-live-reload": "^1.10.2",
    "ember-cli-mirage": "^1.1.6",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^2.1.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-router-scroll": "^1.2.1",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^2.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-plugin-ember": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "fake-xml-http-request": "^2.1.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.5",
    "release-it": "^13.0.0",
    "release-it-lerna-changelog": "^2.0.0",
    "sass": "^1.26.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-file-upload/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
