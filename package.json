{
  "name": "ember-file-upload",
  "version": "3.0.6",
  "description": "An uploader component for Ember apps",
  "keywords": [
    "ember-addon",
    "file-upload"
  ],
  "homepage": "https://adopted-ember-addons.github.io/ember-file-upload",
  "repository": "https://github.com/adopted-ember-addons/ember-file-upload",
  "license": "MIT",
  "author": {
    "name": "Alon Bukai",
    "email": "alonzorz@gmail.com",
    "url": "https://github.com/Alonski"
  },
  "contributors": [
    {
      "name": "Tim Evans"
    },
    {
      "name": "Jeldrik Hanschke",
      "url": "https://github.com/jelhan"
    },
    {
      "name": "Giles Thompson",
      "email": "me@gilesthompson.co.nz",
      "url": "http://gilesthompson.co.nz"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "^5.7.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.41.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.0.0",
    "ember-cli": "3.25.1",
    "ember-cli-addon-docs": "^1.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-content-security-policy": "^2.0.0-3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^1.1.6",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "~3.26.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.2.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fake-xml-http-request": "^2.1.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.0.0",
    "release-it-lerna-changelog": "^3.0.0",
    "sass": "^1.32.10",
    "webpack": "5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-file-upload/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
